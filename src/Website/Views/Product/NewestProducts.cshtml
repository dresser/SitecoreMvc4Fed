@using Sitecore.Data.Fields
@model Sitecore.Mvc.Presentation.RenderingModel
<div class="row small-up-2 large-up-4">
    @{ 
        var field = new MultilistField(Model.Item.Fields["Products"]);
        var productItems = field.GetItems();
    }
    @for (int i = 0; i < productItems.Length; i++)
    {
        @Html.Partial("~/Views/Product/ProductTeaser.cshtml", productItems[i])
    }
</div>
