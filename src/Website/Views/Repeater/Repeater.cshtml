@using Sitecore.Data.Items
@using System.Data.Linq
@model Sitecore.Mvc.Presentation.RenderingModel
@{ 
    var childRenderingId = Model.Rendering.Parameters["ChildRendering"];
    var datasourceItems = (IEnumerable<Item>)Model.Item.Children;
}
@foreach (var item in datasourceItems)
{
    using (Html.Sitecore().SwitchContextItem(item))
    {
        @Html.Sitecore().Rendering(childRenderingId)
    }
}
