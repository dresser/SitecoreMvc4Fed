@using Sitecore.Foundation.FedEx
@model Sitecore.Mvc.Presentation.RenderingModel
<div class="blog-post">
    <h3>@Html.Sitecore().Field("SectionHeader") <small>@Html.Sitecore().FormattedDateTimeField("Date", "dd MMM yyyy")</small></h3>
    @Html.Sitecore().Field("Image")
    <p>@Html.Sitecore().Field("SectionContent")</p>
</div>
